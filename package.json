{
    "name": "discord-http-kit",
    "version": "1.0.0",
    "description": "A lightweight HTTP-based Discord bot framework built with @discordjs/core and discord-api-types for self, designed to simplify the development process by streamlining command creation and API integration.",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "lint": "eslint . --ext .ts,.js",
        "lint:fix": "eslint . --ext .ts,.js --fix",
        "prettier": "prettier --write .",
        "prettier:check": "prettier --check .",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./builders": {
            "import": "./dist/builders/index.js",
            "require": "./dist/builders/index.js",
            "types": "./dist/builders/index.d.ts"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Youzi9601/discord-http-kit.git"
    },
    "author": "Youzi9601",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/Youzi9601/discord-http-kit/issues"
    },
    "homepage": "https://github.com/Youzi9601/discord-http-kit#readme",
    "dependencies": {
        "@discordjs/builders": "^1.10.1",
        "@discordjs/collection": "^2.1.1",
        "@discordjs/core": "^2.0.1",
        "@discordjs/util": "^1.1.1",
        "discord-api-types": "^0.38.1",
        "tweetnacl": "^1.0.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.25.1",
        "eslint": "^9.25.1",
        "eslint-config-prettier": "^10.1.2",
        "eslint-plugin-prettier": "^5.2.6",
        "globals": "^16.0.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.31.0"
    }
}
